{

  "manifest_version": 2,
  "name": "Poof",
  "version": "1.0",
  "default_title": "Find my SR",

  "description": "Adds a border around all Screen Reader elements",

  "icons": {
    "48": "icons/audioEmpty.svg"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/audioEmpty.svg",
    "default_title": "QA Ninja?!",
    "default_popup": "popup/pop.html"
  }
}