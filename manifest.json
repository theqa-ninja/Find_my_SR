{

  "manifest_version": 2,
  "name": "Poof",
  "version": "1.0",
  "default_title": "Find my SR",

  "description": "Adds a border around all Screen Reader elements",

  "icons": {
    "48": "icons/audio.svg"
  },

  "browser_action": {
    "default_icon": "icons/audio.svg",
    "default_title": "QA Ninja?!"
  },

  "content_scripts": [
  {
    "matches": ["*://*.bing.com/*"],
    "js": ["borderify.js"]
  }]
}