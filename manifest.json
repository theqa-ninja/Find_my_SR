{

  "manifest_version": 2,
  "name": "Poof",
  "version": "1.0",
  "default_title": "Find my SR",

  "description": "Do you pass Screen Reader checks?  Want to know what the Screen Reader will read? I gotchu!",

  "icons": {
    "48": "icons/audioEmpty.svg"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/audioEmpty.svg",
    "default_title": "Find my SR!",
    "default_popup": "popup/pop.html"
  }
}